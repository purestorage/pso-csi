kind: StorageClass
apiVersion: storage.k8s.io/v1
metadata:
  name: pure-file-exportrules
  labels:
    kubernetes.io/cluster-service: "true"
    generator: helm
    chart: pure-csi
    release: "pure-storage-driver"
provisioner: pure-csi
parameters:
    backend: file
    exportrules: "*(rw)"
mountOptions:
      - tcp
      - nfsvers=4.1
